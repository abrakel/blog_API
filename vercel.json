{
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "source": "/(.*)"
    },
    {
      "source": "/create"
    },
    {
      "source": "/articles/:ultimos?",
      "destination": "/articles"
    },
    {
      "source": "/article/:id",
      "destination": "/article"
    },
    {
      "source": "/upload/:id",
      "destination": "/upload"
    },
    {
      "source": "/image/:file",
      "destination": "/image"
    },
    {
      "source": "/search/:files",
      "destination": "/search"
    }
  ]
}
